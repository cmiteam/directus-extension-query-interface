{
  "name": "directus-extension-query-interface",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-bundle"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "query-endpoint",
        "source": "src/query-endpoint/index.ts"
      },
      {
        "type": "interface",
        "name": "query-interface",
        "source": "src/query-interface/index.ts"
      }
    ],
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^17.0.7",
    "@types/node": "^25.2.2",
    "@types/sqlstring": "^2.3.2",
    "typescript": "^5.9.3",
    "vue": "^3.5.28"
  },
  "dependencies": {
    "@directus/sdk": "^21.1.0",
    "papaparse": "^5.5.2",
    "sqlstring": "^2.3.3"
  }
}
